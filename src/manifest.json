{
  "manifest_version": 2,

  "name": "BloCKEditor",
  "description": "Replaces Blogger's HTML editor with a real HTML Editor backed by CKEditor",
  "version": "1.1",
  "homepage_url": "https://github.com/ajbogh/BloCKEditor",
  "permissions": [
    "activeTab"
  ],

  "content_scripts": [{
    "matches": ["https://www.blogger.com/*"],
    "js": ["contentScript.js"],
    "css" : ["lib/ckeditor/ckeditor.css"]
  }],
  "web_accessible_resources" : [
      "initiator.js", 
      "lib/Scroll.js", 
      "lib/ckeditor/config.js", 
      "lib/ckeditor/ckeditor.js", 
      "lib/ckeditor/skins/moono/editor.css", 
      "lib/ckeditor/skins/moono/dialog.css",
      "lib/ckeditor/skins/moono/icons.png",
      "lib/ckeditor/skins/moono/icons_hidpi.png",
      "lib/ckeditor/skins/moono/images/arrow.png",
      "lib/ckeditor/skins/moono/images/close.png",
      "lib/ckeditor/skins/moono/images/lock-open.png",
      "lib/ckeditor/skins/moono/images/lock.png",
      "lib/ckeditor/skins/moono/images/refresh.png",
      "lib/ckeditor/skins/moono/images/hidpi/close.png",
      "lib/ckeditor/skins/moono/images/hidpi/lock-open.png",
      "lib/ckeditor/skins/moono/images/hidpi/lock.png",
      "lib/ckeditor/skins/moono/images/hidpi/refresh.png",
      "lib/ckeditor/lang/en.js", 
      "lib/ckeditor/styles.js", 
      "lib/ckeditor/plugins/sourcedialog/plugin.js",
      "lib/ckeditor/plugins/sourcedialog/lang/en.js",
      "lib/ckeditor/plugins/sourcedialog/icons/sourcedialog.png",
      "lib/ckeditor/plugins/sourcedialog/icons/hidpi/sourcedialog.png",
      "lib/ckeditor/plugins/sourcedialog/dialogs/sourcedialog.js",
      "lib/ckeditor/plugins/showblocks/images/block_address.png",
      "lib/ckeditor/plugins/showblocks/images/block_blockquote.png",
      "lib/ckeditor/plugins/showblocks/images/block_div.png",
      "lib/ckeditor/plugins/showblocks/images/block_h1.png",
      "lib/ckeditor/plugins/showblocks/images/block_h2.png",
      "lib/ckeditor/plugins/showblocks/images/block_h3.png",
      "lib/ckeditor/plugins/showblocks/images/block_h4.png",
      "lib/ckeditor/plugins/showblocks/images/block_h5.png",
      "lib/ckeditor/plugins/showblocks/images/block_h6.png",
      "lib/ckeditor/plugins/showblocks/images/block_p.png",
      "lib/ckeditor/plugins/showblocks/images/block_pre.png",
      "lib/ckeditor/plugins/div/dialogs/div.js",
      "lib/ckeditor/plugins/link/dialogs/anchor.js",
      "lib/ckeditor/plugins/link/dialogs/link.js",
      "lib/ckeditor/plugins/link/images/anchor.png",
      "lib/ckeditor/plugins/link/images/hidpi/anchor.js",
      "lib/ckeditor/plugins/image/images/noimage.png",
      "lib/ckeditor/plugins/image/dialogs/image.js",
      "lib/ckeditor/plugins/table/dialogs/table.js",
      "lib/ckeditor/plugins/magicline/images/icon.png",
      "lib/ckeditor/plugins/magicline/images/hidpi/icon.png",
      "lib/ckeditor/plugins/specialchar/dialogs/specialchar.js",
      "lib/ckeditor/plugins/specialchar/dialogs/lang/en.js",
      "lib/ckeditor/plugins/pagebreak/images/pagebreak.gif",
      "lib/ckeditor/plugins/colordialog/dialogs/colordialog.js",
      "lib/ckeditor/plugins/clipboard/dialogs/paste.js",
      "lib/ckeditor/plugins/pastefromword/filter/default.js",
      "lib/ckeditor/plugins/find/dialogs/find.js",
      "lib/ckeditor/plugins/scayt/dialogs/options.js",
      "lib/ckeditor/plugins/scayt/dialogs/toolbar.css",
      "lib/ckeditor/plugins/wsc/dialogs/ciframe.html",
      "lib/ckeditor/plugins/wsc/dialogs/tmp.html",
      "lib/ckeditor/plugins/wsc/dialogs/tmpFrameset.html",
      "lib/ckeditor/plugins/wsc/dialogs/wsc.css",
      "lib/ckeditor/plugins/wsc/dialogs/wsc.js",
      "lib/ckeditor/plugins/wsc/dialogs/inline.js",
      "lib/ckeditor/plugins/wsc/dialogs/inline_tmp.js"
  ]
}
